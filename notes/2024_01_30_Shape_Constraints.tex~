  \begin{align*}
    E[\text{size of Q at time t}] = & \text{Current size of queue} + \\
                                    & E[\text{Number of new arrivals by time t}] - \\
                                    & E[\text{Number of service completions by time t, given no breaks in service}]
  \end{align*}

